    "clean": "rimraf ./npm-debug.log ./coverage/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jstrimpel/chai-string.git"
  },
  "author": "Jason Strimpel <jstrimpel@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jstrimpel/chai-string/issues"
  },
  "homepage": "https://github.com/jstrimpel/chai-string#readme",
  "devDependencies": {
    "devtool": "^2.2.0",
    "eslint": "^3.3.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.0.2",
    "rimraf": "^2.5.4"
  },
  "dependencies": {
    "chai": "^3.5.0"
  }
}
